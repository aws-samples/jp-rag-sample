{
  "api": {
    "fargate": {
      "apiType": "REST",
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "ClusterName",
            "VpcId",
            "VpcCidrBlock",
            "SubnetIds",
            "VpcLinkId",
            "CloudMapNamespaceId"
          ],
          "category": "",
          "resourceName": "NetworkStack"
        },
        {
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb"
          ],
          "category": "auth",
          "resourceName": "jpragsampleamplify"
        }
      ],
      "deploymentMechanism": "FULLY_MANAGED",
      "iamAccessUnavailable": true,
      "imageSource": {
        "type": "CUSTOM"
      },
      "providerPlugin": "awscloudformation",
      "resourceName": "fargate",
      "restrictAccess": false,
      "service": "ElasticContainer",
      "skipHashing": false
    }
  },
  "auth": {
    "jpragsampleamplify": {
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "TOTP"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_UPPERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS"
          ],
          "passwordPolicyMinLength": "8"
        },
        "signupAttributes": [
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito",
      "serviceType": "managed"
    }
  },
  "function": {},
  "hosting": {
    "amplifyhosting": {
      "providerPlugin": "awscloudformation",
      "service": "amplifyhosting",
      "type": "manual"
    }
  }
}